{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "ignorePatterns": [
    "examples/*.ts",
    "dist/",
    "dist_esm/",
    "rollup.config.mjs"
  ],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": 0,
    "require-jsdoc": 0,
    "linebreak-style": 0,
    "no-unused-vars": 0,
    "@typescript-eslint/no-namespace": 0,
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "max-len": [
      "error",
      120
    ]
  }
}